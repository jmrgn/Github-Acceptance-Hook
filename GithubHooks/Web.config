<configuration>
  <configSections>
    <section name="RepositoryConfiguration" type="GithubHooks.Configuration.RepositoryConfiguration" />
    <section name="ApiCredentialsConfiguration" type="GithubHooks.Configuration.ApiCredentialsConfiguration" />
    <section name="AllowedMergersConfiguration" type="GithubHooks.Configuration.AllowedMergersConfiguration" />
  </configSections>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="WebGrease" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.5.2.14234" newVersion="1.5.2.14234" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <RepositoryConfiguration configSource="Configuration\Repository.config" />
  <ApiCredentialsConfiguration configSource="Configuration\ApiCredentials.config" />
  <AllowedMergersConfiguration configSource="Configuration\AllowedMergers.config" />
</configuration>